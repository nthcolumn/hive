#------------------------------------------------
# Makefile for arm
#------------------------------------------------
ARCH=arm
TOOLCHAIN=/opt/arm-devel
BIN=$(TOOLCHAIN)/usr/bin
HEADERS=$(TOOLCHAIN)/include
LIBS=$(TOLLCHAIN)/lib
CC=$(BIN)/gcc
AR=${BIN}ar
RANLIB=${BIN}ranlib

CFLAGS  += -I$(HEADERS) -L$(LIBS)
CFLAGS  += -D_MIPS_SZPTR=32 -DLINUX -DMIPS_LE

OFLAGS	= -Os
